public class Main {
    public static void main(String[] args) {
        bismillah("baku","06 Ocak 2024 Cumartesi\t24 Cemaziyelahir 1445\t06:26\t07:57\t12:51\t15:14\t17:35\t19:00\n" +
                "07 Ocak 2024 Pazar\t25 Cemaziyelahir 1445\t06:26\t07:57\t12:51\t15:15\t17:36\t19:01\n" +
                "08 Ocak 2024 Pazartesi\t26 Cemaziyelahir 1445\t06:26\t07:57\t12:52\t15:16\t17:37\t19:02\n" +
                "09 Ocak 2024 Salı\t27 Cemaziyelahir 1445\t06:26\t07:56\t12:52\t15:17\t17:38\t19:03\n" +
                "10 Ocak 2024 Çarşamba\t28 Cemaziyelahir 1445\t06:26\t07:56\t12:53\t15:17\t17:39\t19:04\n" +
                "11 Ocak 2024 Perşembe\t29 Cemaziyelahir 1445\t06:26\t07:56\t12:53\t15:18\t17:40\t19:04\n" +
                "12 Ocak 2024 Cuma\t1 Recep 1445\t06:26\t07:56\t12:53\t15:19\t17:41\t19:05\n" +
                "13 Ocak 2024 Cumartesi\t2 Recep 1445\t06:26\t07:56\t12:54\t15:20\t17:42\t19:06\n" +
                "14 Ocak 2024 Pazar\t3 Recep 1445\t06:26\t07:55\t12:54\t15:21\t17:43\t19:07\n" +
